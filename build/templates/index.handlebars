  <div class="container main">
    <dl id="filter" class="filter">
      <dt>Filter:</dt>
      <dd class="active"><a href="#" data-filter="*">All Themes</a></dd>
      <dd><a href="#" data-filter=".dark">Dark</a></dd>
      <dd><a href="#" data-filter=".light">Light</a></dd>
    </dl>
    <ul id="thumbnails" class="thumbnails">
      {{#themes}}
      <li class="span4 item {{lightness}}">
         <div class="thumbnail">
          <a href="{{snake_case}}/"><img src="img/{{snake_case}}-clipped.png" width="288" height="183" alt="{{name}}"></a>
          <div class="caption">
            <h4>{{name}}</h4>
          </div>
        </div>
      </li>
      {{/themes}}
    </ul>
  </div>
  
