class Person
  include ActiveModel::Conversion
  include ActiveMode::Validations
  
  validates_presence_of :name
  
  attr_accessor :name
  
  def initialize(attributes = {})
    @name = attributes[:name]
  end
  
  def persist
    @persisted = true
  end
  
  def persisted?
    @persisted
  end
  
  person1 = Person.new(:name => "mats")
  p person1.valid?
