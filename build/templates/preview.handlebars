<head>
  <meta charset="utf-8">
  <link rel="stylesheet" href="{{uri_root}}/css/themes/{{snake_case}}.css">
  <style type="text/css">
  html, body {
    margin: 0;
    padding: 0;
  }
  pre.prettyprint {
    margin: 0;
    border: none;
  }
  </style>
</head>
<body class="google-code-prettify">
<pre class="prettyprint lang-coffee">
class Animal
  constructor: (@name) ->

  move: (meters) ->
    alert @name + " moved #{meters}m."

class Snake extends Animal
  move: ->
    alert "Slithering..."
    super 5

class Horse extends Animal
  move: ->
    alert "Galloping..."
    super 45

sam = new Snake "Sammy the Python"
tom = new Horse "Tommy the Palomino"

sam.move()
tom.move()
</pre>
<script src="{{uri_root}}/js/prettify.js"></script>
<script src="{{uri_root}}/js/lang-css.js"></script>
<script>
(function() {
  if (window.prettyPrint) {
    prettyPrint();
  }
})();
</script>
</body>
</html>
